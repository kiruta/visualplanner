<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Visual Planner</title>  

  <script>
  window.Planner = {
      isLogged: <%= auth %>
    <% if (user) { %>
    , user: {
        id: '<%= user._id %>'
      , name: '<%= user.name %>'
      , email: '<%= user.email %>'
    }
    <% } %>
  };
  </script>

  <% if (env == 'production') { %>
    <link rel="stylesheet" href="/dist/main.css">
    <script src="/vendors/jquery/jquery.min.js" type="text/javascript"></script>
    <script src="/dist/main.min.js" type="text/javascript"></script>
  <% } else { %>
    <link rel="stylesheet/less" type="text/css" href="/less/main.less">
    <script src="/vendors/jquery/jquery.js" type="text/javascript"></script>
    <script src="/vendors/less.js/dist/less-1.3.3.js" type="text/javascript"></script>
    <script data-main="/js/main" src="/vendors/requirejs/require.js"></script>
  <% } %>
</head>
<body id="fn-application">
  <div class="navbar navbar-inverse navbar-fixed-top">
    <div class="navbar-inner">
      <div class="header-container">
        <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="brand" href="/" data-type="app-link" title="planto.co"><img src="/images/logo.png" /></a>
        <div class="nav-collapse collapse">
          <div class="fn-user-side"></div>          
        </div><!--/.nav-collapse -->
      </div>
    </div>
  </div>

  <div class="site-body" id="fn-page-wraper">

  </div>

  <div class="popup-wrapper hide" id="popup-wrapper"></div>
  <div class="utilities-wrapper" id="utilities-wrapper"></div>
   <footer>
        <div class="container">
          <div class="row">
            <div class="span7">
              <h3 class="footer-title">Subscribe early!</h3>
              <p>And get exclusive bonuses after launch!<br><br><br><br><br>
                Have any related ideas or comments?<br>
                Feel free to contact us in social networks or e-mail
              </p>

              

            </div> <!-- /span8 -->

            <div class="span5">
              <div class="footer-banner">
                <h3 class="footer-title">Social media</h3>
                <ul>
                  <li><a href="https://www.facebook.com/plantoplanning">Facebook</a></li>
                  <li><a href="#">https://twitter.com/Plantoco</a></li>
                  <li><a href="#">Google Plus</a></li>
                    
                </ul>
                e-mail: info@planto.co</a>
              </div>
            </div>
          </div>
        </div>
      </footer>
</body>
</html>